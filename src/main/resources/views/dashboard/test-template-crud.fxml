<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="590.0" prefWidth="950.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.the.hugging.team.controllers.Test">
    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="590.0" prefWidth="750.0" style="-fx-background-color: blue;"
                AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="200.0"
                AnchorPane.topAnchor="0.0">
        <TextField fx:id="searchField" layoutX="14.0" layoutY="23.0" prefHeight="30.0" prefWidth="250.0"
                   AnchorPane.bottomAnchor="548.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="495.0"
                   AnchorPane.topAnchor="10.0"/>
        <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="10.0"
                   AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="50.0">
            <columns>
                <TableColumn fx:id="firstName" prefWidth="205.0" text="First name"/>
                <TableColumn fx:id="lastName" minWidth="0.0" prefWidth="307.0" text="Last name"/>
                <TableColumn fx:id="sex" minWidth="0.0" prefWidth="218.0" text="Sex"/>
            </columns>
        </TableView>
        <Button layoutX="274.0" layoutY="13.0" mnemonicParsing="false" onAction="#search" prefHeight="30.0"
                prefWidth="60.0" text="Търси" AnchorPane.bottomAnchor="548.0" AnchorPane.leftAnchor="270.0"
                AnchorPane.rightAnchor="425.0" AnchorPane.topAnchor="10.0"/>
    </AnchorPane>
    <AnchorPane layoutX="749.0" minHeight="0.0" minWidth="0.0" prefHeight="590.0" prefWidth="200.0"
                AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="750.0" AnchorPane.rightAnchor="0.0"
                AnchorPane.topAnchor="0.0">
        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="50.0"
              style="-fx-background-color: orange;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="151.0" text="Добавяне"/>
            <Button mnemonicParsing="false" onAction="#edit" prefHeight="50.0" prefWidth="151.0"
                    text="Редактиране"/>
            <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="151.0" text="Разглеждане"/>
        </VBox>
    </AnchorPane>
</AnchorPane>
